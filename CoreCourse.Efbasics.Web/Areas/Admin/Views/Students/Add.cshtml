@model StudentsAddViewModel
<div class="row">
    <div class="col col-lg-4 mx-auto border border-1">
        <h2>@ViewBag.PageTitle</h2>
        <form asp-area="Admin" asp-controller="Students" asp-action="Add"
              method="post" enctype="multipart/form-data">
            <partial name="_studentFormPartial" model="@Model" />

            <button type="submit" class="btn btn-primary">Add</button>
        </form>
    </div>
</div>


